<h1>Listing saysomethings</h1>

<table>
  <tr>
    <th>Headlines</th>
    <th>Say</th>
    <th>Interests</th>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @saysomethings.each do |saysomething| %>
  <tr>
    <td><%= saysomething.headlines %></td>
    <td><%= saysomething.say %></td>
    <td><%= saysomething.interests %></td>
    <td><%= saysomething.user_id %></td>
    <td><%= link_to 'Show', saysomething %></td>
    <td><%= link_to 'Edit', edit_saysomething_path(saysomething) %></td>
    <td><%= link_to 'Destroy', saysomething, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

